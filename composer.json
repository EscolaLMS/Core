{
  "name": "escolalms/core",
  "description": "Escola Headless LMS",
  "type": "package",
  "require": {
    "php": ">=7.4",
    "bensampo/laravel-enum": ">=2.0",
    "composer/composer": "^2.0",
    "laravel/framework": ">=7.0",
    "laravel/passport": ">=9.0",
    "spatie/laravel-permission": ">=3.18"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": ">=5.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Marcin Lenkowski",
      "email": "marcin.lenkowski@escola.pl"
    },
    {
      "name": "Mateusz Wojczal",
      "email": "mateusz.wojczal@escola.pl"
    },
    {
      "name": "Paweł Chołaj",
      "email": "pawel.cholaj@escolasoft.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\Core\\": "src",
      "EscolaLms\\Core\\Tests\\": "tests",
      "Database\\Factories\\EscolaLms\\Core\\Models\\": "database/factories",
      "EscolaLms\\Core\\Database\\Seeders\\": "database/seeders"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\Core\\EscolaLmsServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
